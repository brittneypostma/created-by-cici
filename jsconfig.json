{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
    "baseUrl": "",
    "paths": {
      "$lib":["src/lib"],
      "$lib/*":["src/lib/*"],
      "@icons": ["src/lib/icons"],
      "@images": ["src/lib/images"],
			"@lib": ["src/lib/"],
      "@lib/*": ["src/lib/*"],
			"@components": ["src/lib/components"],
			"@utils": ["src/lib/utils"]
		}
}
